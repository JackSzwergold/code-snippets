{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true
  },
  "include": [
    "js",
    ".eslintrc.js",
    "gulpfile.babel.ts"
  ],
  "exclude": [
    "js/min",
    "node_modules"
  ]
}
